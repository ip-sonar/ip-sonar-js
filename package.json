{
  "name": "@ip-sonar/ip-sonar-js",
  "version": "1.0.2",
  "description": "IP-Sonar.com JS SDK",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ip",
    "geolocation",
    "api",
    "sdk",
    "typescript",
    "browser"
  ],
  "author": "IP-Sonar.com",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "rollup": "^4.6.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.32.1"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ip-sonar/ip-sonar-js.git"
  },
  "bugs": {
    "url": "https://github.com/ip-sonar/ip-sonar-js/issues"
  },
  "homepage": "https://github.com/ip-sonar/ip-sonar-js#readme"
}
